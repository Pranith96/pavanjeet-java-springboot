public class MainMethod {

	public static void main(String[] args) {
		AdditionImpl additionImpl = new AdditionImpl();  // compile time
		additionImpl.add(5, 5);
		
		Addition ad = new AdditionImpl(); //runtime polymorphism
		ad.add(10, 10);
		
		
		Addition ad1 = (x,y) -> {
			
		};
		ad1.add(15, 10);
	}
}
